http://www.khelll.com/blog/mysql/changing-database-encoding-from-latin-to-utf8/

mysqldump --default-character-set=latin1 --skip-set-charset -d -uusername -ppassword db_name > db_name_schema.sql

mysqldump --default-character-set=latin1 --skip-set-charset -t -uusername -ppassword  db_name > db_name_data.sql


mysql --default-character-set=utf8 -u root -p speedcrete < speed_schema.sql
mysql --default-character-set=utf8  -u root -p speedcrete < speed_data.sql


mysql -u root -p -e 'CREATE DATABASE speedcrete DEFAULT CHARACTER SET utf8 COLLATE utf8_general_ci;'

sed "s/latin1/utf8/g" speed_schema.sql > speed_schema2.sql
